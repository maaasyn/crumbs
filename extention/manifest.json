{
    "manifest_version": 3,
    "name": "Scrumbs",
    "version": "1.0",
    "description": "Fck the cookies, only scrumbs.",
    "web_accessible_resources": [
        {
            "resources": [
                "icons/maskot.svg"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "permissions": [
        "clipboardWrite",
        "notifications",
        "storage",
        "https://*/*",
        "http://*/*",
        "activeTab",
        "alarms",
        "clipboardWrite",
        "notifications",
        "tabs",
        "scripting",
        "webNavigation",
        "storage",
        "webRequest",
        "browserAction"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/all-sizes/favicon-16x16.png",
            "32": "icons/all-sizes/favicon-32x32.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "icons": {
        "16": "icons/all-sizes/favicon-16x16.png",
        "32": "icons/all-sizes/favicon-32x32.png"
    }
}